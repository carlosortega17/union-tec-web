{{> logged }}
<div class="container" style="margin-top: 76px">
  {{#if user}}
  <div class="card">
    <div class="card-header">
      <h5>Nombre completo: {{user.fullname}}</h5>
    </div>
    <div class="card-body">
      Contacto: <a href="mailto:{{user.email}}">{{user.email}}</a><br>
      Se registro: {{formatDate user.createdAt}}<br>
      Numero de publicaciones: {{numPosts}}
    </div>
  </div>
  {{/if}}
</div>